fib_part = p.(
    Pair[Println[Second[p]], Add[First[p], Second[p]]]
)

fib = n.(
    Second[
        For[n, fib_part, Pair[0, 1]]
        ]
)



main = args.(
    Assert[
        Less[len[args], 1]
        "No argument supplied"
    ]

    Assert[
        Not[IsNumber[Index[args, 0]]]
        "Argument must be an integer"
    ]

    fib[
        StrToNum[Index[args, 0]]
    ]
)
